package vamshi_bhavaniCodeing.com.chandu.in;

import java.util.Scanner;

public class AsciValueOFCharactier {
	@SuppressWarnings("resource")
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Please enter any Charactier");
		String nextLine = scanner.nextLine();
		System.out.println("Given charactier is-->" + nextLine);
		Integer valueOf = Integer.valueOf(nextLine);

		System.out.println("Asci value of the charactier is-->" + valueOf);

	}
}
