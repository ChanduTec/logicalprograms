package vamshi_bhavaniCodeing.com.chandu.in;

public class LargetsAmongThreeNumbers {
	static int arr[] = { 10, 324, 9808, 45,1248, 90 };

	static int largest() {
	
		int max =0;
		for (	int i = 1; i < arr.length; i++)
			if (arr[i] > max)
				max = arr[i];

		return max;
	}

	// Driver method
	public static void main(String[] args) {
		 int arr[] = { 10, 324, 9808, 45,1248, 90 };

		
			int max =0;
			for (	int i = 1; i < arr.length; i++)
				if (arr[i] > max)
					max = arr[i];

		System.out.println(max);
	}
}