package vamshi_bhavaniCodeing.com.chandu.in;

public class LargetsAmongThreeNumbers {
	public static void main(String[] args) {
		int array[] = { 12, 23, 45 };
		int length = array.length;

		int max = 0;
		for (int i = 0; i < length; i++) {
			if (array[i] > 0) {
                    max=array[i];
			}
			
		}
		System.out.println(max);
	}
}
